<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <solid android:color="#f53b2e"/>
    <padding android:top="50dp"
        android:bottom="50dp"
        android:left="50dp"
        android:right="50dp"/>
    <size android:height="200dp"
        android:width="200dp"/>

    <corners
         android:radius="50dp"
       />
    <stroke android:color="#0011ff"
        android:width="7dp"
        android:dashGap="15dp"
        android:dashWidth="25dp"/>
    <gradient
        android:angle="0"
        android:startColor="#ff0"
        android:endColor="#232623"

        />

</shape>
